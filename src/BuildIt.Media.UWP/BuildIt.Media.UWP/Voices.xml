<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-au" Name="PlayerCommandset_en-au">
    <AppName> Player Sample </AppName>
    <Example> Play / Pause / Go forward / Go back </Example>

    <Command Name="buildit_pause">
      <Example> Pause </Example>
      <ListenFor> pause </ListenFor>
      <Feedback> Pausing </Feedback>
      <Navigate />
    </Command>

    <Command Name="buildit_play">
      <Example> Play </Example>
      <ListenFor> play </ListenFor>
      <ListenFor> resume </ListenFor>
      <Feedback> Resuming </Feedback>
      <!-- Description: this is description for play -->
      <Navigate />
    </Command>
    <Command Name="buildit_forward">
      <Example> Forward </Example>
      <ListenFor> [go] forward </ListenFor>
      <Feedback> Seeking forward... </Feedback>
      <!-- Description: this is description for forward -->
      <Navigate />
    </Command>
    <Command Name="buildit_back">
      <Example> Back </Example>
      <ListenFor> [go] back </ListenFor>
      <Feedback> Seeking backward... </Feedback>
      <!-- Description:this is description for back -->
      <Navigate />
    </Command>
    <Command Name="buildit_volumeUp">
      <Example> Volume Up </Example>
      <ListenFor> [volume] up </ListenFor>
      <Feedback> Switching up volume... </Feedback>
      <!-- Description:this is description for volume up -->
      <Navigate/>
    </Command>
    <Command Name="buildit_volumeDown">
      <Example> Volume Down </Example>
      <ListenFor> [volume] down </ListenFor>
      <Feedback> Switching down volume... </Feedback>
      <!-- Description:this is description for volume down -->
      <Navigate/>
    </Command>
    <Command Name="buildit_mute">
      <Example> Mute </Example>
      <ListenFor> [volume] mute </ListenFor>
      <Feedback> Muting volume... </Feedback>
      <!-- Description:this is description for mute -->
      <Navigate/>
    </Command>
    <Command Name="buildit_unmute">
      <Example> Unmute </Example>
      <ListenFor> [volume] unmute </ListenFor>
      <Feedback> Unmuting volume... </Feedback>
      <!-- Description:this is description for unmute -->
      <Navigate/>
    </Command>
    <Command Name="buildit_help">
      <Example> Help </Example>
      <ListenFor> help </ListenFor>
      <Feedback> Here is the help list for you. </Feedback>
      <VoiceCommandService Target="test"/>
    </Command>
  </CommandSet>
</VoiceCommands>